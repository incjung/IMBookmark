{
  "manifest_version": 3,
  "name": "IMBookmark",
  "version": "1.0",
  "description": "Search your bookmarks by keywords from the page content.",
  "permissions": [
    "bookmarks",
    "storage",
    "activeTab",
    "scripting",
    "unlimitedStorage",
    "omnibox"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "omnibox": {
    "keyword": "myurl"
  }
}